<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="minami.com.echplus.mapper.UserMapper">

	<select id="getUsers1" resultType="minami.com.echplus.domain.UserDomain" parameterType="java.lang.Integer">
		SELECT row, id, enrollmentId, token
		FROM Users
		WHERE <![CDATA[ row <= #{row} ]]>
	</select>

</mapper>